{
  "name": "KeyboardShortcuts",
  "version": "0.1",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "content_scripts": [{
     "matches": ["http://*/*", "https://*/*"],
     "js": ["jquery.min.js", "jquery.hotkeys.min.js", "keyboard_shortcuts.js"]
   }],
   "permissions": [
      "activeTab"
   ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}